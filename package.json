{
  "name": "linter-elixirc",
  "version": "1.1.11",
  "linter-package": true,
  "main": "./lib/init",
  "description": "Bare Minimum Elixir lint package",
  "repository": "https://github.com/AtomLinter/linter-elixirc",
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "package-deps": [
    "linter",
    "language-elixir"
  ],
  "dependencies": {
    "atom-linter": "^8.0.0",
    "atom-package-deps": "^4.0.1"
  },
  "devDependencies": {
    "eslint": "^3.13.0",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "global-require": "off",
      "import/no-unresolved": [
        "error",
        {
          "ignore": [
            "atom"
          ]
        }
      ]
    },
    "globals": {
      "atom": true
    },
    "env": {
      "node": true,
      "browser": true
    }
  }
}
